{
  "name": "back_prisma_tests",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prisma-prod": "prisma migrate deploy",
    "prisma-test": "dotenv -e .env.test prisma migrate reset --skip--seed",
    "build": "tsc && npm run prisma-prod",
    "start": "npm build && node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "dev:test": "npm run prisma-test && dotenv -e .env.test nodemon src/server.ts",
    "test": "npm run prisma-test && npm run test-options dotenv -e .env.test jest -- -i -- app.test.ts",
    "test:unit": "npm run test-options jest -- -i --coverage --collectCoverageFrom=src/services/ -- unit.test.ts",
    "test-options": "NODE_OPTIONS=--experimental-vm-modules"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "bcrypt": "^5.0.1",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
